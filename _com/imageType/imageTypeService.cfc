<cfcomponent name="imageTypeService" output="false">	<cffunction name="init" access="public" output="false" returntype="_com.imageType.imageTypeService">		<cfargument name="imageTypeDAO" type="_com.imageType.imageTypeDAO" required="true" />		<cfargument name="imageTypeGateway" type="_com.imageType.imageTypeGateway" required="true" />		<cfset variables.imageTypeDAO = arguments.imageTypeDAO />		<cfset variables.imageTypeGateway = arguments.imageTypeGateway />		<cfreturn this/>	</cffunction>	<cffunction name="getimageType" access="public" output="false" returntype="imageType">		<cfargument name="id" type="numeric" required="true" />				<cfset var imageType = createObject("component","_com.imageType.imageType").init(argumentCollection=arguments) />		<cfif imageType.getimageTypeID() eq 0>			<cfreturn imageType />		<cfelse>			<cfreturn variables.imageTypeDAO.read(imageType) />		</cfif>	</cffunction>	<cffunction name="getimageTypes" access="public" output="false" returntype="query">		<cfargument name="id" type="numeric" required="false" />		<cfargument name="name" type="string" required="false" />				<cfreturn variables.imageTypeGateway.getByAttributes(argumentCollection=arguments) />	</cffunction>	<cffunction name="saveimageType" access="public" output="false" returntype="boolean">		<cfargument name="id" type="numeric" required="true" />		<cfargument name="name" type="string" required="true" />						<cfset var imageType = getimageType(arguments.id) />		<cfset imageType.setid(arguments.id) />		<cfset imageType.setname(arguments.name) />		<cfreturn variables.imageTypeDAO.save(imageType) />	</cffunction>	<cffunction name="deleteimageType" access="public" output="false" returntype="boolean">		<cfargument name="id" type="numeric" required="true" />				<cfset var imageType = createObject("component","_com.imageType.imageType").init(argumentCollection=arguments) />		<cfreturn variables.imageTypeDAO.delete(imageType) />	</cffunction></cfcomponent>
